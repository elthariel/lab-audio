@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.

<http://lab-audio.googlecode.com/files/turntable>
  a lv2:Plugin;
  doap:name "My Plugin";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  ll:gtk2Gui <myplugin_gtk.so>;
  ll:pegName "peg";

  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype <http://ll-plugins.nongnu.org/lv2/ext/miditype>;
    lv2:index 0;
    lv2:symbol "midi";
    lv2:name "MIDI";
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "gain";
    lv2:name "Gain";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 0.5;
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "output";
    lv2:name "Output";
  ].


