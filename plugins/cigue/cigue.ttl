@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.

<http://lab-audio.googlecode.com/svn/trunk/plugins/cigue>
a lv2:Plugin;
doap:name "cigue";
doap:licence <http://usefulinc.com/doap/licenses/gpl>;
ll:gtk2Gui <cigue_gtk.so>;
lv2:requiredHostFeature ll:instrument-ext;
ll:pegName "peg";

lv2:port  
[
a lv2:AudioRateOutputPort;
lv2:datatype lv2:float;
lv2:index 0;
lv2:symbol "main_out";
lv2:name "main_out";
],

[
a lv2:ControlRateInputPort;
lv2:datatype <http://ll-plugins.nongnu.org/lv2/ext/miditype>;
lv2:index 1;
lv2:symbol "midi";
lv2:name "Midi_port";
],

[
a lv2:ControlRateInputPort;
lv2:datatype lv2:float;
lv2:index 2;
lv2:symbol "pitch";
lv2:name "Osc_pitch";
lv2:minimum -0.5;
lv2:maximum 0.5;
lv2:default 0;
],

[
a lv2:ControlRateInputPort;
lv2:datatype lv2:float;
lv2:index 3;
lv2:symbol "cutoff";
lv2:name "Filter_cutoff";
lv2:minimum 10;
lv2:maximum 20000;
lv2:default 20000;
],

[
a lv2:ControlRateInputPort;
lv2:datatype lv2:float;
lv2:index 4;
lv2:symbol "res";
lv2:name "Filter_res";
lv2:minimum 0;
lv2:maximum 15;
lv2:default 2;
],

[
a lv2:ControlRateInputPort;
lv2:datatype lv2:float;
lv2:index 5;
lv2:symbol "env_mod";
lv2:name "Envelop_amount";
lv2:minimum 0;
lv2:maximum 20000;
lv2:default 4000;
],

[
a lv2:ControlRateInputPort;
lv2:datatype lv2:float;
lv2:index 6;
lv2:symbol "env_decay";
lv2:name "Envelop_decay";
lv2:minimum 0;
lv2:maximum 1;
lv2:default 0.5;
],

[
a lv2:ControlRateInputPort;
lv2:datatype lv2:float;
lv2:index 7;
lv2:symbol "glide";
lv2:name "Glide";
lv2:minimum 1;
lv2:maximum 1000;
lv2:default 200;
].


