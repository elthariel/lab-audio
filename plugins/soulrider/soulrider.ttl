@prefix lv2:  <http://lv2plug.in/ontology#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix ll: <http://ll-plugins.nongnu.org/lv2/namespace#>.
@prefix mm: <http://ll-plugins.nongnu.org/lv2/ext/midimap#>.

<http://lab-audio.googlecode.com/svn/trunk/plugins/soulrider>
  a lv2:Plugin;
  doap:name "Soulrider";
  doap:licence <http://usefulinc.com/doap/licenses/gpl>;
  ll:gtk2Gui <soulrider_gtk.so>;
  lv2:requiredHostFeature ll:instrument-ext;
  ll:pegName "peg";

  lv2:port [
    a lv2:ControlRateInputPort;
    lv2:datatype <http://ll-plugins.nongnu.org/lv2/ext/miditype>;
    lv2:index 0;
    lv2:symbol "midi";
    lv2:name "MIDI";
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 1;
    lv2:symbol "pitch";
    lv2:name "Pitch";
    lv2:minimum 0.1;
    lv2:maximum 2.0;
    lv2:default 1.0;
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 2;
    lv2:symbol "play";
    lv2:name "Play";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 0;
    lv2:portHint lv2:toggled;
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 3;
    lv2:symbol "pause";
    lv2:name "Pause";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 0;
    lv2:portHint lv2:toggled;
  ],

  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 4;
    lv2:symbol "cue";
    lv2:name "Cue";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 0;
    lv2:portHint lv2:toggled;
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 5;
    lv2:symbol "output_l";
    lv2:name "Output_l";
  ],

  [
    a lv2:AudioRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 6;
    lv2:symbol "output_r";
    lv2:name "Output_r";
  ],
  [
    a lv2:ControlRateInputPort;
    lv2:datatype lv2:float;
    lv2:index 7;
    lv2:symbol "position";
    lv2:name "Position";
    lv2:minimum 0.0;
    lv2:maximum 100.0;
    lv2:default 0.0;
  ],
  [
    a lv2:ControlRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 8;
    lv2:symbol "current_position";
    lv2:name "Current Position";
    lv2:minimum 0.0;
    lv2:maximum 100.0;
    lv2:default 0.0;
  ],
  [
    a lv2:ControlRateOutputPort;
    lv2:datatype lv2:float;
    lv2:index 9;
    lv2:symbol "beatsmasher_length";
    lv2:name "Beatsmasher Length";
    lv2:minimum 0.0;
    lv2:maximum 44100.0;
    lv2:default 0.0;
  ]
  .


